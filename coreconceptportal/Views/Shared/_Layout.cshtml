    <script src="/Content/assets/js/bootstrap.min.js"></script>
    <script src="/Content/assets/js/perfect-scrollbar.js"></script>
    <script src="/Content/assets/js/jquery-ui.min.js"></script>
    <!-- Global Required Scripts End -->
